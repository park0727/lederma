---
import { languages } from "../../../i18n/ui";
import HospitalPage from "../../../components/pages/intro/Hospital.astro";

export function getStaticPaths() {
    return Object.keys(languages)
        .map((lang) => {
            if (lang === "ko") return undefined;
            return { params: { lang } };
        })
        .filter(Boolean);
}
---

<HospitalPage />
